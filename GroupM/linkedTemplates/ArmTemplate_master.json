{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "GroupM"
		},
		"ExcelSupplyChainLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ExcelSupplyChainLS'"
		},
		"OutputSQLDBLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OutputSQLDBLS'"
		},
		"ExcelSupplyChainLS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\CASE_STUDY Project 1\\DATA\\"
		},
		"ExcelSupplyChainLS_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Lenovo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "GroupM_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"OutputSQLDBLS_connectionString": {
						"value": "[parameters('OutputSQLDBLS_connectionString')]"
					},
					"ExcelSupplyChainLS_password": {
						"value": "[parameters('ExcelSupplyChainLS_password')]"
					},
					"ExcelSupplyChainLS_properties_typeProperties_host": {
						"value": "[parameters('ExcelSupplyChainLS_properties_typeProperties_host')]"
					},
					"ExcelSupplyChainLS_properties_typeProperties_userId": {
						"value": "[parameters('ExcelSupplyChainLS_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}